    "scripts": {
        "lint": "tsc",
        "start": "cross-env ENV_LWD=development DOMAIN=ikandy webpack-dev-server --open --config script/webpack.dev.js",
        "build:test": "cross-env ENV_LWD=test DOMAIN=ikandy webpack --config script/webpack.prod.js",
        "build:prod": "cross-env ENV_LWD=production DOMAIN=ikandy webpack --config script/webpack.prod.js",
        "build:uat-test": "cross-env ENV_LWD=test DOMAIN=uat webpack --config script/webpack.prod.js",
        "build:uat-prod": "cross-env ENV_LWD=production DOMAIN=uat webpack --config script/webpack.prod.js",
        "watch": "webpack --watch --config script/webpack.dev.js",
        "script-name": "babel-node script.js",
        "lint:jsx": "eslint --ext .jsx src && eslint --ext .js src",
        "lint:css": "stylelint --aei .less .css src",
        "precommit": "lint-staged",
        "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
    },
    "pre-commit": [
        "precommit",
        "precommit-msg"
    ],
    "lint-staged": {
        "*.{js,jsx}": [
            "eslint --fix",
            "prettier --write",
            "git add"
        ],
        "*.{css,less}": [
            "stylelint --fix",
            "prettier --write",
            "git add"
        ]
    },
